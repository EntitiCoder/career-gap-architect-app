{
  "name": "career-gap-architect-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "docker-compose up",
    "dev:web": "cd apps/web && npm run dev",
    "dev:api": "cd apps/api && npm run dev",
    "build": "npm run build --workspaces",
    "clean": "rm -rf node_modules apps/*/node_modules apps/*/.next"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
